#include <stdio.h>

int main()
{
    int numbers[5];
    int i;
    int max;

    printf("Enter 5 numbers:\n");
    for(i = 0; i < 5; i++)
    {
        printf("Enter number %d: ", i + 1);
        scanf("%d", &numbers[i]);
    }

    max = numbers[0];

    for(i = 1; i < 5; i++)
    {
        if(numbers[i] > max)
        {
            max = numbers[i];
        }
    }

    printf("The maximum number in the array is: %d\n", max);

    return 0;
}

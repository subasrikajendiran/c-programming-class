#include <stdio.h>

int main() {
    int n, i, marks, total = 0;

    printf("Enter number of students: ");
    scanf("%d", &n);

    for(i = 1; i <= n; i++) {
        printf("Enter marks for student %d: ", i);
        scanf("%d", &marks);
        total += marks;
    }

    printf("Total Marks of %d students = %d\n", n, total);

    return 0;
}

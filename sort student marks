#include <stdio.h>

int main() {
    int n, i, j, temp;
    
    printf("Enter number of students: ");
    scanf("%d", &n);

    int marks[n];

    for(i = 0; i < n; i++) {
        printf("Enter marks of student %d: ", i + 1);
        scanf("%d", &marks[i]);
    }

    for(i = 0; i < n - 1; i++) {
        for(j = i + 1; j < n; j++) {
            if(marks[i] > marks[j]) {
                temp = marks[i];
                marks[i] = marks[j];
                marks[j] = temp;
            }
        }
    }

    printf("\nSorted marks (ascending order):\n");
    for(i = 0; i < n; i++) {
        printf("%d ", marks[i]);
    }
    printf("\n");

    return 0;
}
